body{
	font-family:"microsoft yahei", 'Helvetica Neue', Helvetica, sans-serif;
	font-weight: 300;
}
#myMain .mui-navigate-right {
	height: 52px;
}

#myMain span {
	position: relative;
	top: -2px;
	left: 10px;
	font-size: 16px;
	color: #333;
}

#myMain .row {
	background: #fff;
	margin: 0;
	padding-bottom: 20px;
}

#myMain .row .mui-col-xs-6 {
	text-align: left;
	padding-left: 30px;
	border: none;
	color: #666;
}

#myMain .row .mui-col-xs-6 strong {
	color: #000;
	font-weight: 400;
	position: relative;
	top: 6px;
	font-size: 16px;
}

#myMain .list-block {
	margin-top: 15px;
	background: #fff;
	margin-bottom: 15px;
}

#myMain .list-block img {
	width: 26px;
	margin-right: 10px;
	margin-left: 10px;
}

#myMain .list-block a {
	font-size: 14px;
}

#myMain {
	background: #f0f0f0;
	padding: 0;
}

#myMain .content-block-title {
	background: #fff;
	margin: 0;
	padding: 15px 30px;
	line-height: 20px;
}

#myMain .mui-table-view .mui-media-object {
	width: 20px;
	height: 20px;
	margin-top: 7px;
}

#myMain .mui-table-view .mui-media-body {
	margin-top: 7px;
	color: #666;
	font-size: 14px;
}

#tab-4 * {
	box-sizing: border-box;
	outline: none;
}

#tab-4 .head {
	background-image:linear-gradient(-180deg,#2e9fd7,#2264d5);
	height: 240px;
	/*position: fixed;*/
	z-index: 2;
	width: 100%;
	position: relative;
	/* transition: all 1s; */
	top: 0px;
	overflow: hidden;
}

#tab-4 .head .user_pic {
	display: block;
	width: 56px;
	position: absolute;
	left: calc(50% - 28px);
	top: 50px;
	/* transition: all 1s; */
}

#tab-4 .head .user_name {
	    color: #fff;
    font-size: 14px;
    display: block;
    text-align: center;
    position: absolute;
    top: 115px;
    width: 100%;
	/* transition: all 1s; */
}

#tab-4 .head .user_name2 {
	color: #fff;
	font-size: 16px;
	display: block;
	text-align: center;
	position: absolute;
	top: 120px;
	width: 100%;
	/* transition: all 1s; */
}

.user_data>div {
	background: rgba(0, 0, 0, 0);
	border: none;
}

.user_data {
	color: #fff !important;
	text-align: center;
	position: absolute;
	margin: 0px;
	left: 0px;
	right: 0px;
	top: 165px;
	/* transition: all 1s; */
}

.user_data>div>span {
	color: #fff !important;
	display: block;
	font-size: 18px;
	transition: all 1s;
}

.user_data>div>i {
	color: #fff !important;
	display: block;
	font-style: normal;
	font-size: 12px;
	transition: all 1s;
}

.user_data>div>a {
	transition: all 1s;
	color: #fff !important;
	display: block;
	font-size: 12px;
	background: rgba(255, 255, 255, 0.2);
	width: 60px;
	text-align: center;
	border: #fff solid 1px;
	border-radius: 20px;
	margin: 10px auto 0;
}

#tab-4.ontop .head {
	height: 115px;
}

#tab-4.ontop .head .user_pic {
	width: 50px;
	left: 10px;
	top: 30px;
}

#tab-4.ontop .head .user_name {
	top: 81px;
	width: 72px;
}

#tab-4.ontop .user_data>div {}

#tab-4.ontop .user_data {
	top: 34px;
	left: 70px;
}

#tab-4.ontop .user_data>div>span {
	font-size: 12px;
}

#tab-4.ontop .user_data>div>i {
	font-size: 10px;
}

#tab-4.ontop .user_data>div>a {
	font-size: 10px;
	margin: 5px auto 0;
	width: 50px;
	height: 20px;
}

#tab-4 {
	background: #f5f5f5 !important;
}

#myNav {
	background: #fff !important;
	text-align: center;
	/*padding-top: 248px;*/
	transition: all 1s;
}

#tab-4.ontop #myNav {
	background: #fff !important;
	text-align: center;
	padding-top: 115px;
}

#myNav .mui-row {
	padding-top: 10px !important;
	padding-bottom: 10px !important;
}

#myNav .mui-row>div {
	border: none;
	font-size: 12px;
	padding: 10px !important;
	margin: 0px;
	text-align: center;
}

#myNav img {
	display: block;
	margin: 5px auto;
	width: 30px;
}

#myNav a {
	color: #666 !important;
	display: block !important;
	padding: 0px;
}

#myAct .title {
	height: 20px;
	padding: 5px;
	margin-bottom: 5px;
}

#myAct .title img {
	float: left;
	height: 20px;
	margin-right: 10px;
}

#myAct .mui-row>div {
	border: none;
	padding: 5px;
}

#myAct img {
	display: block;
	margin: 0
}
