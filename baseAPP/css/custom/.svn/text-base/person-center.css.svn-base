.yuanhu{ position: absolute; bottom:0px;  width: 100%; height: 20px; background: url(img/yuanhu.png) no-repeat; background-size: 100% 100%;}
/*气泡动画*/
.animateMain{
	border-radius:50%;
	position: absolute;
	background: #fff;
	z-index: 3;
}
.animate1 {
	width: 20px;
	height: 20px;
	left: 10px;
	top:210px;
	opacity: 0.15;
	animation: myfirst1 120s;
	animation-timing-function: linear;
	animation-iteration-count:infinite;
	filter: blur(1px);
}
@keyframes myfirst1 {
	0% {
		opacity: 0.15;
		width: 20px;
		height: 20px;
		top:210px
	}
	50% {
		opacity: 0.05;
		width: 20px;
		height: 20px;
		top:40px
	}
	
	100% {
		opacity: 0.15;
		width: 20px;
		height: 20px;
		top:210px
	}
}
