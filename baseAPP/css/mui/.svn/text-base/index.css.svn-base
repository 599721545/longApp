}

[data-dpr="1"] textarea::-webkit-input-placeholder {
	font-size: 15px
}

[data-dpr="2"] textarea::-webkit-input-placeholder {
	font-size: 30px
}

[data-dpr="3"] textarea::-webkit-input-placeholder {
