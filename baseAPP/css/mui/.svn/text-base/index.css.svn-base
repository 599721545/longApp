.textarea {
	width: 100%;
	height: 3.333333rem;
	padding: .266667rem;
	overflow: visible;
	margin-top: .4rem
}

.select-con,
.textarea {
	border: 1px solid #eee;
	border-radius: .066667rem
}

.select-con {
	padding: .266667rem .4rem;
	position: relative
}

.select-con:after {
	content: '';
	width: .266667rem;
	height: .266667rem;
	border-top: 1px solid #bebebe;
	border-left: 1px solid #bebebe;
	transform: rotate(-135deg);
	-webkit-transform: rotate(-135deg);
	position: absolute;
	right: .4rem
}

.area-tit {
	display: inline-block;
	width: 100%;
	padding: 0 .133333rem;
	background-color: #fff
}

.header-bar>span>a.reg:after {
	content: none
}

.header-bar>a,
.header-bar>span {
	display: inline-block;
	line-height: 1.2rem
}

.login-mask {
	display: none;
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	background-color: #000;
	opacity: .6
}

.login {
	width: 3.2rem;
	height: 3.2rem;
	background: url(data:image/gif;base64,) no-repeat;
	background-size: 100%;
	top: 50%;
	left: 50%;
	margin-left: -1.6rem;
	margin-top: -1.6rem
}

.close,
.login {
	position: absolute
}

.close {
	width: .666667rem;
	height: .666667rem;
	background: url(data:image/png;base64,) no-repeat;
	background-size: 100%;
	top: .4rem;
	right: .4rem
}

a.font-white:hover,
a.font-white:link,
a.font-white:visited {
	color: #fff!important
}

.pwd-eye {
	width: 1.413333rem;
	background-size: .56rem auto!important;
	background-color: #fff;
	background-position: 50%;
	background-repeat: no-repeat
}

.pwd-eye.eye-open {
	background-image: url(data:image/png;base64,)
}

.pwd-eye.eye-close {
	background-image: url(data:image/png;base64,)
}

.pop_area {
	position: fixed;
	top: 50%;
	left: 50%;
	min-width: 4.266667rem;
	min-height: 1.6rem;
	padding: .533333rem;
	text-align: center;
	background-color: rgba(0, 0, 0, .5);
	border-radius: .266667rem;
	box-sizing: border-box;
	-webkit-transform: translate3d(-50%, -50%, 0);
	transform: translate3d(-50%, -50%, 0)
}

.pop_area i {
	margin: 0 auto .213333rem
}

.pop_area p {
	color: #fff
}

.pop_area.pop_success i {
	display: block;
	width: .8rem;
	height: .8rem;
	background: transparent url(data:image/png;base64,) 50%;
	background-size: .8rem .8rem
}