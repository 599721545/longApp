header.header {
	padding: .4rem;
	border-bottom: .013333rem solid #d6d6d6;
	background-color: #f8f8f8;
	position: relative
}

header.header>i {
	display: inline-block;
	width: .266667rem;
	height: .266667rem;
	border-top: 1px solid #000;
	border-left: 1px solid #000;
	-webkit-transform: rotate(-45deg);
	position: relative;
	top: .133333rem
}

.nav-btn {
	display: block
}

.form {
	padding: .266667rem
}

.input-item {
	padding-bottom: .266667rem
}

.input-box {
	padding: .48rem .4rem;
	background-color: #fff;
	border-radius: .066667rem
}

.input-box input {
	width: 100%;
	border-width: 0
}

.jyts {
	padding: 0 0 .266667rem .666667rem;
	padding: 0 0 .266667rem
}

.btn {
	padding: .48rem 0;
	border-radius: .066667rem;
	cursor: pointer
}

.btn.disable {
	color: #d0d0d0
}

.pwd-btn {
	border-left: 1px solid #eee;
	background-color: #fff
}

.eye {
	display: inline-block;
	width: .4rem;
	height: .253333rem;
	background: url(data:image/png;base64,) no-repeat;
	background-size: 100%;
	position: relative;
	top: .266667rem
}

[data-dpr="1"] .font-btn {
	font-size: 14px
}

[data-dpr="2"] .font-btn {
	font-size: 28px
}

[data-dpr="3"] .font-btn {
	font-size: 42px
}

[data-dpr="1"] .font-32 {
	font-size: 16px
}

[data-dpr="2"] .font-32 {
	font-size: 32px
}

[data-dpr="3"] .font-32 {
